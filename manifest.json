{
  "manifest_version": 3,
  "name": "Tatkal ticket booking",
  "version": "1.0",
  "description": "Right-side panel like sider",
  "permissions": ["sidePanel", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Side Panel"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
